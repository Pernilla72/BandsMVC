@model BandsMVC.Models.Band
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<!DOCTYPE html>
    <html>
    <head>
        <title>@Model.Name</title>
    </head>
    <body>
           <h1>@Model.Name</h1>
           <p>@Model.Description</p>
    <img src="@Url.Content($"~/{Model.ImgUrl}")" alt="Alternate Text" />
    <p>
        <h1>One famous tune from this band</h1>
        <iframe width="560" height="315"
                src="https://www.youtube.com/embed/@Model.YoutubeUrl"
                frameborder="0"
                allowfullscreen>
        </iframe>
    </p>
    </body>
    </html>